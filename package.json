{
  "name": "my-webpack-cli",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "cross-env NODE_ENV=development BUILD_ENV=dev webpack-dev-server --config ./build/webpack.dev.config.js",
    "build": "npm run build:dev",
    "build:dev": "cross-env NODE_ENV=production BUILD_ENV=dev webpack ./build/webpack.prod.config.js",
    "build:uat": "cross-env NODE_ENV=production BUILD_ENV=uat webpack ./build/webpack.prod.config.js",
    "build:prod": "cross-env NODE_ENV=production BUILD_ENV=dev webpack ./build/webpack.prod.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
