{
  "name": "my-webpack-cli",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "cross-env NODE_ENV=development BUILD_ENV=dev webpack-dev-server --config ./build/webpack.dev.config.js",
    "build": "npm run build:dev",
    "build:dev": "cross-env NODE_ENV=production BUILD_ENV=dev webpack --config ./build/webpack.prod.config.js",
    "build:uat": "cross-env NODE_ENV=production BUILD_ENV=uat webpack --json > stats.json --config ./build/webpack.prod.config.js",
    "build:prod": "cross-env NODE_ENV=production BUILD_ENV=dev webpack --json > stats.json --config ./build/webpack.prod.config.js",
    "build:dll": "webpack --config ./build/webpack.dll.config.js",
    "analyze": "webpack-bundle-analyzer ./stats.json",
    "server": "cross-env http-server ./dist -p 8090"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@types/handlebars": "^4.0.40",
    "@types/lodash": "^4.14.121",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autoprefixer": "^9.4.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "handlebars": "^4.1.0",
    "jquery": "^3.3.1"
  }
}
